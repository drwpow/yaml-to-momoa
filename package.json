{
  "name": "yaml-to-momoa",
  "version": "0.0.2",
  "description": "Convert YAML to Momoa JSON AST",
  "type": "module",
  "author": {
    "name": "Drew Powers",
    "email": "drew@pow.rs"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/drwpow/yaml-to-momoa"
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "del-cli dist && tsc -p tsconfig.build.json",
    "lint": "biome check src",
    "format": "biome format src --write",
    "test": "pnpm run --parallel \"/^test:*/\"",
    "test:ts": "tsc --noEmit",
    "test:js": "vitest run",
    "version": "pnpm run build && changeset version && pnpm i"
  },
  "dependencies": {
    "@humanwhocodes/momoa": "^3.3.0",
    "yaml": "^2.6.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.9",
    "@types/node": "^22.7.9",
    "del-cli": "^5.1.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  }
}
